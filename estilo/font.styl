@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?48s5le');
	src:url('../fonts/icomoon.eot?#iefix48s5le') format('embedded-opentype'),
		url('../fonts/icomoon.woff?48s5le') format('woff'),
		url('../fonts/icomoon.ttf?48s5le') format('truetype'),
		url('../fonts/icomoon.svg?48s5le#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-facebook:before {
	content: "\face";
}

.icon-twitter:before {
	content: "\e604";
}
